<h1>Listing logs</h1>

<table>
  <tr>
  	<th>Id</th>
    <th>Activity</th>
    <th>Memos</th>
    <th>Created at</th>
  </tr>

<% @logs.each do |log| %>
  <tr>
  	<td><%= log.id %></td>
    <td><%= link_to log.activity, log %></td>
    <td><%= log.memos %></td>
    <td><%= log.created_at %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Log', new_log_path %>
